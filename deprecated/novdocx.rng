<grammar xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <define name="admonitions">
    <choice>
      <ref name="important"/>
      <ref name="note"/>
      <ref name="tip"/>
      <ref name="warning"/>
    </choice>
  </define>
  <define name="lists">
    <choice>
      <ref name="itemizedlist"/>
      <ref name="orderedlist"/>
      <ref name="simplelist"/>
      <ref name="variablelist"/>
      <ref name="calloutlist"/>
    </choice>
  </define>
  <define name="tables">
    <choice>
      <ref name="table"/>
      <ref name="informaltable"/>
    </choice>
  </define>
  <define name="figures">
    <choice>
      <ref name="figure"/>
      <ref name="informalfigure"/>
    </choice>
  </define>
  <!-- CHANGE_SEP_28 added bridgehead to common group and removed the other specific instances -->
  <define name="common">
    <choice>
      <ref name="bridgehead"/>
      <ref name="example"/>
      <ref name="screen"/>
      <ref name="remark"/>
      <ref name="para"/>
      <ref name="formalpara"/>
      <ref name="indexterm"/>
    </choice>
  </define>
  <define name="all">
    <choice>
      <ref name="admonitions"/>
      <ref name="lists"/>
      <ref name="figures"/>
      <ref name="tables"/>
      <ref name="common"/>
    </choice>
  </define>
  <define name="xrefinlines">
    <choice>
      <ref name="xref"/>
      <ref name="link"/>
      <ref name="ulink"/>
    </choice>
  </define>
  <define name="coreinlines">
    <choice>
      <ref name="command"/>
      <ref name="envar"/>
      <ref name="productname"/>
      <ref name="literal"/>
      <ref name="emphasis"/>
      <ref name="option"/>
      <ref name="replaceable"/>
      <ref name="remark"/>
      <ref name="filename"/>
      <ref name="citetitle"/>
      <ref name="quote"/>
      <ref name="phrase"/>
      <ref name="systemitem"/>
      <ref name="guimenu"/>
      <ref name="subscript"/>
      <ref name="superscript"/>
      <ref name="trademark"/>
      <ref name="varname"/>
    </choice>
  </define>
  <define name="basicinlines">
    <choice>
      <ref name="coreinlines"/>
      <ref name="indexterm"/>
    </choice>
  </define>
  <define name="guiinlines">
    <choice>
      <ref name="keycombo"/>
      <ref name="keycap"/>
      <ref name="menuchoice"/>
    </choice>
  </define>
  <define name="allinlines">
    <choice>
      <ref name="xrefinlines"/>
      <ref name="basicinlines"/>
      <ref name="guiinlines"/>
      <ref name="inlinemediaobject"/>
    </choice>
  </define>
  <!-- Profiling information (attributes) -->
  <define name="profiling.attributes">
    <optional>
      <attribute name="os"/>
    </optional>
    <optional>
      <attribute name="arch"/>
    </optional>
    <optional>
      <attribute name="condition"/>
    </optional>
  </define>
  <define name="xmlbase">
    <optional>
      <attribute name="xml:base"/>
    </optional>
  </define>
  <define name="set">
    <element name="set">
      <ref name="attlist.set"/>
      <optional>
        <ref name="title"/>
        <optional>
          <ref name="subtitle"/>
        </optional>
        <optional>
          <ref name="titleabbrev"/>
        </optional>
      </optional>
      <optional>
        <ref name="toc"/>
      </optional>
      <oneOrMore>
        <ref name="book"/>
      </oneOrMore>
      <optional>
        <ref name="setindex"/>
      </optional>
    </element>
  </define>
  <define name="book">
    <element name="book">
      <ref name="attlist.book"/>
      <ref name="bookinfo"/>
      <optional>
        <ref name="preface"/>
      </optional>
      <oneOrMore>
        <choice>
          <ref name="part"/>
          <ref name="chapter"/>
          <ref name="article"/>
        </choice>
      </oneOrMore>
      <zeroOrMore>
        <choice>
          <ref name="appendix"/>
          <ref name="glossary"/>
        </choice>
      </zeroOrMore>
      <optional>
        <ref name="index"/>
      </optional>
    </element>
  </define>
  <!-- change require title in bookinfo  this will be full book title used for links to bookinfo but not displayed -->
  <define name="bookinfo">
    <element name="bookinfo">
      <ref name="attlist.bookinfo"/>
      <ref name="title"/>
      <ref name="productname"/>
      <optional>
        <ref name="productnumber"/>
      </optional>
      <optional>
        <ref name="date"/>
      </optional>
      <optional>
        <ref name="titleabbrev"/>
      </optional>
      <oneOrMore>
        <ref name="legalnotice"/>
      </oneOrMore>
      <zeroOrMore>
        <ref name="authorgroup"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="abstract"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="article">
    <element name="article">
      <ref name="attlist.article"/>
      <ref name="title"/>
      <optional>
        <ref name="subtitle"/>
      </optional>
      <optional>
        <ref name="articleinfo"/>
      </optional>
      <choice>
        <group>
          <oneOrMore>
            <choice>
              <ref name="calloutlist"/>
              <ref name="itemizedlist"/>
              <ref name="orderedlist"/>
              <ref name="variablelist"/>
              <ref name="important"/>
              <ref name="note"/>
              <ref name="tip"/>
              <ref name="warning"/>
              <ref name="screen"/>
              <ref name="formalpara"/>
              <ref name="para"/>
              <ref name="mediaobject"/>
              <ref name="informaltable"/>
              <ref name="example"/>
              <ref name="figure"/>
              <ref name="table"/>
              <ref name="procedure"/>
              <ref name="bridgehead"/>
              <ref name="remark"/>
              <ref name="abstract"/>
              <ref name="indexterm"/>
            </choice>
          </oneOrMore>
          <choice>
            <zeroOrMore>
              <ref name="sect1"/>
            </zeroOrMore>
            <zeroOrMore>
              <ref name="refentry"/>
            </zeroOrMore>
          </choice>
        </group>
        <choice>
          <oneOrMore>
            <ref name="sect1"/>
          </oneOrMore>
          <oneOrMore>
            <ref name="refentry"/>
          </oneOrMore>
        </choice>
      </choice>
      <zeroOrMore>
        <choice>
          <ref name="index"/>
          <ref name="glossary"/>
          <ref name="appendix"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="articleinfo">
    <element name="articleinfo">
      <ref name="attlist.articleinfo"/>
      <oneOrMore>
        <choice>
          <ref name="author"/>
          <ref name="authorgroup"/>
          <ref name="date"/>
          <ref name="productname"/>
          <ref name="productnumber"/>
          <ref name="legalnotice"/>
          <ref name="indexterm"/>
          <ref name="abstract"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="preface">
    <element name="preface">
      <ref name="attlist.preface"/>
      <ref name="title"/>
      <choice>
        <group>
          <oneOrMore>
            <choice>
              <ref name="all"/>
              <ref name="procedure"/>
            </choice>
          </oneOrMore>
          <zeroOrMore>
            <ref name="sect1"/>
          </zeroOrMore>
        </group>
        <oneOrMore>
          <ref name="sect1"/>
        </oneOrMore>
      </choice>
    </element>
  </define>
  <define name="part">
    <element name="part">
      <ref name="attlist.part"/>
      <ref name="title"/>
      <optional>
        <ref name="partintro"/>
      </optional>
      <choice>
        <group>
          <oneOrMore>
            <ref name="chapter"/>
          </oneOrMore>
          <zeroOrMore>
            <ref name="appendix"/>
          </zeroOrMore>
        </group>
        <oneOrMore>
          <ref name="appendix"/>
        </oneOrMore>
      </choice>
    </element>
  </define>
  <!-- CHANGE_SEP_28 added msgset to Appendix, Chapter and Sect1 -->
  <define name="appendix">
    <element name="appendix">
      <ref name="attlist.appendix"/>
      <ref name="title"/>
      <oneOrMore>
        <choice>
          <ref name="all"/>
          <ref name="procedure"/>
        </choice>
      </oneOrMore>
      <choice>
        <zeroOrMore>
          <ref name="sect1"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="refentry"/>
        </zeroOrMore>
        <optional>
          <ref name="msgset"/>
        </optional>
      </choice>
    </element>
  </define>
  <define name="chapter">
    <element name="chapter">
      <ref name="attlist.chapter"/>
      <ref name="title"/>
      <oneOrMore>
        <choice>
          <ref name="abstract"/>
          <ref name="all"/>
          <ref name="procedure"/>
        </choice>
      </oneOrMore>
      <choice>
        <zeroOrMore>
          <ref name="sect1"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="refentry"/>
        </zeroOrMore>
        <optional>
          <ref name="msgset"/>
        </optional>
      </choice>
    </element>
  </define>
  <define name="abstract">
    <element name="abstract">
      <ref name="attlist.abstract"/>
      <optional>
        <ref name="title"/>
      </optional>
      <oneOrMore>
        <ref name="para"/>
      </oneOrMore>
    </element>
  </define>
  <define name="index">
    <element name="index">
      <ref name="attlist.index"/>
      <empty/>
    </element>
  </define>
  <define name="setindex">
    <element name="setindex">
      <ref name="attlist.setindex"/>
      <empty/>
    </element>
  </define>
  <define name="toc">
    <element name="toc">
      <ref name="attlist.toc"/>
      <empty/>
    </element>
  </define>
  <define name="attlist.toc" combine="interleave">
    <empty/>
  </define>
  <!-- sections -->
  <define name="sect1">
    <element name="sect1">
      <ref name="attlist.sect1"/>
      <ref name="title"/>
      <oneOrMore>
        <choice>
          <ref name="all"/>
          <ref name="procedure"/>
        </choice>
      </oneOrMore>
      <choice>
        <zeroOrMore>
          <ref name="sect2"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="refentry"/>
        </zeroOrMore>
        <optional>
          <ref name="msgset"/>
        </optional>
      </choice>
    </element>
  </define>
  <define name="sect2">
    <element name="sect2">
      <ref name="attlist.sect2"/>
      <ref name="title"/>
      <oneOrMore>
        <choice>
          <ref name="all"/>
          <ref name="procedure"/>
        </choice>
      </oneOrMore>
      <zeroOrMore>
        <ref name="sect3"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="sect3">
    <element name="sect3">
      <ref name="attlist.sect3"/>
      <ref name="title"/>
      <oneOrMore>
        <choice>
          <ref name="all"/>
          <ref name="procedure"/>
        </choice>
      </oneOrMore>
      <zeroOrMore>
        <ref name="sect4"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="sect4">
    <element name="sect4">
      <ref name="attlist.sect4"/>
      <ref name="title"/>
      <oneOrMore>
        <choice>
          <ref name="all"/>
          <ref name="procedure"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="partintro">
    <element name="partintro">
      <ref name="attlist.partintro"/>
      <oneOrMore>
        <ref name="all"/>
      </oneOrMore>
      <zeroOrMore>
        <ref name="sect1"/>
      </zeroOrMore>
    </element>
  </define>
  <!-- inlines -->
  <define name="application">
    <element name="application">
      <ref name="attlist.application"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="phrase"/>
          <ref name="remark"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="command">
    <element name="command">
      <ref name="attlist.command"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="option"/>
          <ref name="replaceable"/>
          <ref name="remark"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="envar">
    <element name="envar">
      <ref name="attlist.envar"/>
      <text/>
    </element>
  </define>
  <define name="literal">
    <element name="literal">
      <ref name="attlist.literal"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="replaceable"/>
          <ref name="remark"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="emphasis">
    <element name="emphasis">
      <ref name="attlist.emphasis"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="xrefinlines"/>
          <ref name="basicinlines"/>
          <ref name="guiinlines"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="citetitle">
    <element name="citetitle">
      <ref name="attlist.citetitle"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="phrase"/>
          <ref name="remark"/>
          <ref name="xrefinlines"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="option">
    <element name="option">
      <ref name="attlist.option"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="replaceable"/>
          <ref name="remark"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="replaceable">
    <element name="replaceable">
      <ref name="attlist.replaceable"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="remark"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="filename">
    <element name="filename">
      <ref name="attlist.filename"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="replaceable"/>
          <ref name="remark"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <!-- CHANGE quote can contain other inlines -->
  <define name="quote">
    <element name="quote">
      <ref name="attlist.quote"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="allinlines"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="phrase">
    <element name="phrase">
      <ref name="attlist.phrase"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="allinlines"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="productname">
    <element name="productname">
      <ref name="attlist.productname"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="phrase"/>
          <ref name="remark"/>
          <ref name="superscript"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="productnumber">
    <element name="productnumber">
      <ref name="attlist.productnumber"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="phrase"/>
          <ref name="remark"/>
          <ref name="superscript"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="systemitem">
    <element name="systemitem">
      <ref name="attlist.systemitem"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="remark"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="varname">
    <element name="varname">
      <ref name="attlist.varname"/>
      <text/>
    </element>
  </define>
  <define name="attlist.varname" combine="interleave">
    <empty/>
  </define>
  <!-- Curtis, do we need that according to legal issues? -->
  <define name="trademark">
    <element name="trademark">
      <ref name="attlist.trademark"/>
      <text/>
    </element>
  </define>
  <define name="co">
    <element name="co">
      <ref name="attlist.co"/>
      <empty/>
    </element>
  </define>
  <define name="xref">
    <element name="xref">
      <ref name="attlist.xref"/>
      <empty/>
    </element>
  </define>
  <define name="link">
    <element name="link">
      <ref name="attlist.link"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="basicinlines"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="ulink">
    <element name="ulink">
      <ref name="attlist.ulink"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="basicinlines"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="keycombo">
    <element name="keycombo">
      <ref name="attlist.keycombo"/>
      <oneOrMore>
        <ref name="keycap"/>
      </oneOrMore>
    </element>
  </define>
  <define name="keycap">
    <element name="keycap">
      <ref name="attlist.keycap"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="remark"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="menuchoice">
    <element name="menuchoice">
      <ref name="attlist.menuchoice"/>
      <optional>
        <ref name="shortcut"/>
      </optional>
      <oneOrMore>
        <ref name="guimenu"/>
      </oneOrMore>
    </element>
  </define>
  <!-- toms -->
  <define name="shortcut">
    <element name="shortcut">
      <ref name="attlist.shortcut"/>
      <choice>
        <ref name="keycombo"/>
        <oneOrMore>
          <ref name="keycap"/>
        </oneOrMore>
      </choice>
    </element>
  </define>
  <define name="guimenu">
    <element name="guimenu">
      <ref name="attlist.guimenu"/>
      <text/>
    </element>
  </define>
  <define name="subscript">
    <element name="subscript">
      <ref name="attlist.subscript"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="remark"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="superscript">
    <element name="superscript">
      <ref name="attlist.superscript"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="remark"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <!-- common -->
  <define name="bridgehead">
    <element name="bridgehead">
      <ref name="attlist.bridgehead"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="coreinlines"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="title">
    <element name="title">
      <ref name="attlist.title"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="coreinlines"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="subtitle">
    <element name="subtitle">
      <ref name="attlist.subtitle"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="coreinlines"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.subtitle" combine="interleave">
    <empty/>
  </define>
  <define name="titleabbrev">
    <element name="titleabbrev">
      <ref name="attlist.titleabbrev"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="coreinlines"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="example">
    <element name="example">
      <ref name="attlist.example"/>
      <ref name="title"/>
      <oneOrMore>
        <choice>
          <ref name="lists"/>
          <ref name="formalpara"/>
          <ref name="para"/>
          <ref name="screen"/>
          <ref name="indexterm"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="para">
    <element name="para">
      <ref name="attlist.para"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="allinlines"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="formalpara">
    <element name="formalpara">
      <ref name="attlist.formalpara"/>
      <ref name="title"/>
      <ref name="para"/>
    </element>
  </define>
  <!-- change add replaceable, command, emphasis, option  and xrefinlines to screen -->
  <define name="screen">
    <element name="screen">
      <ref name="attlist.screen"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="co"/>
          <ref name="emphasis"/>
          <ref name="replaceable"/>
          <ref name="command"/>
          <ref name="option"/>
          <ref name="phrase"/>
          <ref name="xrefinlines"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="remark">
    <element name="remark">
      <ref name="attlist.remark"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="allinlines"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <!-- admonitions -->
  <define name="admonition.module">
    <optional>
      <ref name="title"/>
    </optional>
    <oneOrMore>
      <choice>
        <ref name="para"/>
        <ref name="remark"/>
        <ref name="screen"/>
        <ref name="simplelist"/>
        <ref name="itemizedlist"/>
        <ref name="variablelist"/>
        <ref name="orderedlist"/>
      </choice>
    </oneOrMore>
  </define>
  <define name="tip">
    <element name="tip">
      <ref name="attlist.tip"/>
      <ref name="admonition.module"/>
    </element>
  </define>
  <define name="important">
    <element name="important">
      <ref name="attlist.important"/>
      <ref name="admonition.module"/>
    </element>
  </define>
  <define name="note">
    <element name="note">
      <ref name="attlist.note"/>
      <ref name="admonition.module"/>
    </element>
  </define>
  <define name="warning">
    <element name="warning">
      <ref name="attlist.warning"/>
      <ref name="admonition.module"/>
    </element>
  </define>
  <!-- lists -->
  <define name="itemizedlist">
    <element name="itemizedlist">
      <ref name="attlist.itemizedlist"/>
      <optional>
        <ref name="title"/>
      </optional>
      <zeroOrMore>
        <ref name="remark"/>
      </zeroOrMore>
      <oneOrMore>
        <ref name="listitem"/>
      </oneOrMore>
    </element>
  </define>
  <define name="orderedlist">
    <element name="orderedlist">
