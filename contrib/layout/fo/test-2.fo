<?xml version="1.0" encoding="UTF-8"?> 
<fo:root xmlns:fo="http://www.w3.org/1999/XSL/Format">
  <fo:layout-master-set>
    <!-- Seiten definieren -->
    <fo:simple-page-master master-name="blank"
       page-height="15cm" page-width="10cm">
      <fo:region-body background-color="gray"/>
    </fo:simple-page-master>
    <fo:simple-page-master master-name="first"
       reference-orientation="-270"
       page-height="15cm" page-width="10cm"
       margin-top="1cm" margin-bottom="1cm"
       margin-left="1cm" margin-right="2cm">
      <fo:region-body margin-bottom="30pt"/>
      <fo:region-after region-name="first-after" extent="30pt" background-color="teal"/>
    </fo:simple-page-master>
    <fo:simple-page-master master-name="odd" 
      page-height="15cm" page-width="10cm"
      margin-top="1cm" margin-bottom="1cm"
      margin-left="1cm" margin-right="2cm">
      <fo:region-body background-color="red" margin-bottom="30pt"/>
      <fo:region-after region-name="odd-after" extent="30pt" background-color="teal"/>
    </fo:simple-page-master>
    <fo:simple-page-master master-name="even"
      page-height="15cm" page-width="10cm"
      margin-top="1cm" margin-bottom="1cm" 
      margin-left="2cm" margin-right="1cm">
      <fo:region-body background-color="green" margin-bottom="30pt"/>
      <fo:region-after region-name="even-after" extent="30pt" background-color="teal"/>
    </fo:simple-page-master>

    <!-- Seiten zusammenfassen -->
    <fo:page-sequence-master master-name="page">
      <fo:repeatable-page-master-alternatives>
        <fo:conditional-page-master-reference master-reference="blank" 
          blank-or-not-blank="blank"/>
        <fo:conditional-page-master-reference master-reference="first"
          page-position="first"/>
        <fo:conditional-page-master-reference master-reference="even"
          odd-or-even="even"/>
        <fo:conditional-page-master-reference master-reference="odd"
          odd-or-even="odd"/>
      </fo:repeatable-page-master-alternatives>
    </fo:page-sequence-master>
  </fo:layout-master-set>

