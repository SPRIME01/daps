<?xml version="1.0"?>


<l:i18n xmlns:l="http://docbook.sourceforge.net/xmlns/l10n/1.0">

<!-- ============================================================= -->
<!-- German                                                        -->
<!-- ============================================================= -->
  <l:l10n language="de">
	<l:gentext key="Author"       text="Autor"/>
	<l:gentext key="Authors"      text="Autoren"/>
	<l:gentext key="Office"       text="Redaktion"/>
	<l:gentext key="Translator"   text="&#xdc;bersetzer"/>
	<l:gentext key="Translators"  text="&#xdc;bersetzer"/>

    <l:context name="keycapfunctions">
      <l:gentext name="alt" text="Alt"/>
      <l:gentext name="backspace" text="$\longleftarrow$"/>
      <l:gentext name="command" text="???"/>
      <l:gentext name="control" text="Strg"/>
      <l:gentext name="delete" text="Entf"/>
      <l:gentext name="down" text="$\downarrow$"/>
      <l:gentext name="end" text="Ende"/>
      <l:gentext name="enter" text="Enter"/>
      <l:gentext name="escape" text="Esc"/>
      <l:gentext name="home" text="Pos~1"/>
      <l:gentext name="insert" text="Einfg"/>
      <l:gentext name="left" text="$\leftarrow$"/>
      <l:gentext name="meta" text="Meta"/>
      <l:gentext name="option" text=""/>
      <l:gentext name="other" text="???"/>
      <l:gentext name="pagedown" text="Bild $\downarrow$"/>
      <l:gentext name="pageup" text="Bild $\uparrow$"/>
      <l:gentext name="right" text="$\leftarrow$"/>
      <l:gentext name="shift" text="Shift $\Uparrow$"/>
      <l:gentext name="space" text="~~~~"/>
      <l:gentext name="tab" text="Tab"/>
      <l:gentext name="up" text="$\uparrow$"/>
    </l:context>

  </l:l10n>

